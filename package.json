{
  "name": "@norges-domstoler/dds-page-generator",
  "version": "6.1.1",
  "description": "Page Generator",
  "author": "Team Spesial",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/page-generator"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "homepage": "https://design.domstol.no",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build": "tsup --dts",
    "build:fast": "tsup",
    "dev": "pnpm build:fast -- --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --c .js,.jsx,.ts,.tsx --config eslint.config.mjs --cache",
    "format:check": "prettier --check src --cache",
    "format:write": "prettier --write src --cache",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "pnpm build && storybook build",
    "serve:storybook": "pnpx serve storybook-static",
    "changeset": "changeset",
    "prerelease": "pnpm build",
    "release": "changeset publish",
    "version:dev": "changeset version --snapshot dev",
    "release:dev": "changeset publish --tag dev"
  },
  "keywords": [
    "page-generator",
    "domstolene",
    "norges domstoler"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@storybook/addon-docs": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-storysource": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/core-common": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.4.7",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@internationalized/date": "^3.6.0",
    "@navikt/fnrvalidator": "^2.1.5",
    "@norges-domstoler/dds-components": "^18.0.0",
    "@norges-domstoler/dds-design-tokens": "^7.0.0",
    "install": "^0.13.0",
    "npm": "^10.9.2",
    "react-select": "^5.9.0"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "publishConfig": {
    "provenance": true
  },
  "volta": {
    "node": "21.5.0",
    "pnpm": "9.12.3"
  }
}
