{
  "name": "@norges-domstoler/dds-page-generator",
  "version": "5.4.1",
  "description": "Page Generator",
  "author": "Team Spesial",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/page-generator"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "homepage": "https://design.domstol.no",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build": "tsup --dts",
    "build:fast": "tsup",
    "dev": "pnpm build:fast -- --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --config .eslintrc.cjs --cache",
    "format:check": "prettier --check src --cache",
    "format:write": "prettier --write src --cache",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "pnpm build && storybook build",
    "serve:storybook": "pnpx serve storybook-static",
    "changeset": "changeset",
    "prerelease": "pnpm build",
    "release": "changeset publish",
    "version:dev": "changeset version --snapshot dev",
    "release:dev": "changeset publish --tag dev"
  },
  "keywords": [
    "page-generator",
    "domstolene",
    "norges domstoler"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@storybook/addon-docs": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-storysource": "^7.6.17",
    "@storybook/blocks": "^7.6.7",
    "@storybook/core-common": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^7.6.7",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.3",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@internationalized/date": "^3.5.4",
    "@navikt/fnrvalidator": "^1.3.0",
    "@norges-domstoler/dds-components": "^16.3.0",
    "@norges-domstoler/dds-design-tokens": "^5.0.0",
    "react-select": "^5.8.0"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "publishConfig": {
    "provenance": true
  },
  "volta": {
    "node": "21.5.0",
    "pnpm": "8.14.1"
  }
}
