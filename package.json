{
  "name": "@norges-domstoler/dds-page-generator",
  "version": "7.3.0",
  "description": "Page Generator",
  "author": "Team Spesial",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/page-generator"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "homepage": "https://design.domstol.no",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build": "tsup --dts",
    "build:fast": "tsup",
    "dev": "npm run build:fast -- --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --c .js,.jsx,.ts,.tsx --config eslint.config.mjs --cache",
    "format:check": "prettier --check src --cache",
    "format:write": "prettier --write src --cache",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "npm run build && storybook build",
    "serve:storybook": "pnpx serve storybook-static",
    "changeset": "changeset",
    "prerelease": "npm run build",
    "release": "changeset publish",
    "version:dev": "changeset version --snapshot dev",
    "release:dev": "changeset publish --tag dev"
  },
  "keywords": [
    "page-generator",
    "domstolene",
    "norges domstoler"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@storybook/addon-docs": "^10.1.2",
    "@storybook/react": "^10.1.2",
    "@storybook/react-vite": "^10.1.2",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.2.0",
    "prettier": "^3.7.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "storybook": "^10.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.2",
    "vite": "^7.2.6",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.34.0",
    "@internationalized/date": "^3.9.0",
    "@navikt/fnrvalidator": "^2.2.0",
    "@types/node": "^24.10.1",
    "install": "^0.13.0",
    "npm": "^11.6.4",
    "react-select": "^5.9.0"
  },
  "peerDependencies": {
    "@norges-domstoler/dds-components": "^21.20.0",
    "@norges-domstoler/dds-design-tokens": "^8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "publishConfig": {
    "provenance": true
  },
  "volta": {
    "node": "22.14.0",
    "npm": "10.9.2"
  }
}
