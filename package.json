{
  "name": "@norges-domstoler/dds-page-generator",
  "version": "8.0.0",
  "description": "Page Generator",
  "author": "Team Spesial",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/page-generator"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "homepage": "https://design.domstol.no",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build": "tsup --dts",
    "build:fast": "tsup",
    "dev": "pnpm run build:fast -- --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --c .js,.jsx,.ts,.tsx --config eslint.config.mjs --cache",
    "format:check": "prettier --check src --cache",
    "format:write": "prettier --write src --cache",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "pnpm run build && storybook build",
    "serve:storybook": "pnpx serve storybook-static",
    "changeset": "changeset",
    "preinstall": "npx only-allow pnpm",
    "prerelease": "pnpm run build",
    "release": "changeset publish",
    "version:dev": "changeset version --snapshot dev",
    "release:dev": "changeset publish --tag dev"
  },
  "keywords": [
    "page-generator",
    "domstolene",
    "norges domstoler"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.34.0",
    "@storybook/react": "^10.2.10",
    "@storybook/react-vite": "^10.2.10",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^24.10.10",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^27.2.0",
    "only-allow": "^1.2.2",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "storybook": "^10.2.10",
    "tsup": "^8.5.1",
    "typescript": "^5.9.2",
    "vite": "^7.2.6",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@internationalized/date": "^3.9.0",
    "@navikt/fnrvalidator": "^2.2.0",
    "react-select": "^5.9.0"
  },
  "peerDependencies": {
    "@norges-domstoler/dds-components": "^22.2.0",
    "@norges-domstoler/dds-design-tokens": "^9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "publishConfig": {
    "provenance": true
  },
  "volta": {
    "node": "22.14.0",
    "pnpm": "10.28.1"
  }
}
